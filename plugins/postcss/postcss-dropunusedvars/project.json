{
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "tags": ["tooling", "postcss"],
    "name": "postcss-dropunusedvars",
	"targets": {
		"build": {
			"executor": "nx:noop"
		},
        "lint": {
            "defaultConfiguration": "js"
        },
        "coverage": {},
        "test": {
            "defaultConfiguration": "plugins",
            "inputs": ["{workspaceRoot}/index.js", "{workspaceRoot}/test.js", "{workspaceRoot}/expected/*.css", "{workspaceRoot}/fixtures/*.css"],
            "options": {
                "cwd": "plugins/postcss/postcss-dropunusedvars"
            }
        }
	}
}
